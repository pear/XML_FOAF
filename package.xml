<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<package version="1.0">
    <!-- $Id: package.xml,v 1.1 2004-01-07 13:03:05 davey Exp $ -->
    <name>XML_FOAF</name>
    <summary>Provides the ability to manipulate FOAF RDF/XML</summary>
    <description>
        XML_FOAF Allows advanced users to create advanced FOAF files.
        The FOAF Project can be found at http://www.foaf-project.org -
        XML_FOAF_Parser and XML_FOAF_Lite will follow before 1.0.
    </description>
    <license>PHP License</license>
    <maintainers>
        <maintainer>
            <user>Davey</user>
            <name>Davey Shafik</name>
            <email>davey@php.net</email>
            <role>lead</role>
        </maintainer>
    </maintainers>
    <release>
        <version>0.1</version>
        <date>2003-01-07</date>
        <state>alpha</state>
        <notes>
            Initial PEAR Release
        </notes>
        <filelist>
            <dir name="/" baseinstalldir="XML">
                <file role="php">FOAF.php</file>
            </dir>
            <dir name="docs">
                <dir name="examples">
                    <file role="doc">example1.php</file>
                </dir>
            </dir>
        </filelist>
    </release>
    <deps>
        <dep type="php" rel="ge">4.3.0</dep>
        <dep type="pkg" rel="ge" version="1.1">XML_Tree</dep>
        <dep type="pkg" rel="ge" version="0.2.2">XML_Beautifier</dep>
    </deps>
</package>